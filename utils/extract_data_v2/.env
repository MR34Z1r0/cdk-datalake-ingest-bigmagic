# config_example.env - Copia esto a .env y ajusta los valores

# ===================
# CONFIGURACIÓN AWS
# ===================
# Opción 1: Usar perfil de AWS CLI (RECOMENDADO)
AWS_PROFILE=prod-compliance-admin
REGION=us-east-1

# Opción 2: Credenciales directas (comentar AWS_PROFILE si usas esto)
# AWS_ACCESS_KEY_ID=tu_access_key_aqui
# AWS_SECRET_ACCESS_KEY=tu_secret_key_aqui
# AWS_DEFAULT_REGION=us-east-1

# ===================
# CONFIGURACIÓN PRINCIPAL
# ===================
PROJECT_NAME=datalake
TEAM=apdayc
DATA_SOURCE=bigmagic
ENDPOINT_NAME=PEBDDATA2
ENVIRONMENT=DEV

# ===================
# ALMACENAMIENTO
# ===================
S3_RAW_BUCKET=sofia-dev-datalake-510543735161-us-east-1-raw-s3
DYNAMO_LOGS_TABLE=sofia-dev-datalake-logs-ddb
TOPIC_ARN=arn:aws:sns:us-east-1:510543735161:sofia-dev-datalake-failed-sns

# ===================
# ARCHIVOS DE CONFIGURACIÓN
# ===================
TABLES_CSV_S3=../../artifacts/configuration/csv/tables.csv
CREDENTIALS_CSV_S3=../../artifacts/configuration/csv/credentials.csv
COLUMNS_CSV_S3=../../artifacts/configuration/csv/columns.csv

# ===================
# CONFIGURACIÓN DE PROCESAMIENTO OPTIMIZADA CENTRALIZADA
# ===================
MAX_THREADS=1
CHUNK_SIZE=500000
OUTPUT_FORMAT=parquet
EXTRACTOR_TYPE=sqlserver
LOADER_TYPE=s3
MONITOR_TYPE=dynamodb
FORCE_FULL_LOAD=false

# ===================
# CONFIGURACIÓN WATERMARK
# ===================
WATERMARK_STORAGE_TYPE=dynamodb
WATERMARK_TABLE=sofia-dev-datalake-watermarks-ddb
WATERMARK_CSV_PATH=../../artifacts/configuration/csv/watermarks.csv
WATERMARK_PG_CONNECTION=
WATERMARK_PG_SCHEMA=watermarks

# ===================
# CONFIGURACIÓN ADICIONAL PARA RESOLVER ERRORES
# ===================
CONNECTION_TIMEOUT=900
LOGIN_TIMEOUT=300
MAX_RETRIES=3
RETRY_DELAY=10
USE_SQLALCHEMY=true
CONNECTION_POOL_SIZE=5
CONNECTION_POOL_RECYCLE=3600

# AWS Step Function Configuration
STEP_FUNCTION_ARN=arn:aws:states:us-east-1:510543735161:stateMachine:sofia-dev-datalake-workflow_instance_bigmagic_pe-sf