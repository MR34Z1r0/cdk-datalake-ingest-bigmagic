# config_example.env - Copia esto a .env y ajusta los valores

# ===================
# CONFIGURACIÓN AWS
# ===================
# Opción 1: Usar perfil de AWS CLI (RECOMENDADO)
AWS_PROFILE=prod-upeu-admin
REGION=us-east-1

# Opción 2: Credenciales directas (comentar AWS_PROFILE si usas esto)
# AWS_ACCESS_KEY_ID=tu_access_key_aqui
# AWS_SECRET_ACCESS_KEY=tu_secret_key_aqui
# AWS_DEFAULT_REGION=us-east-1

# ===================
# CONFIGURACIÓN PRINCIPAL
# ===================
PROJECT_NAME=datalake
TEAM=athenea
DATA_SOURCE=upeu
ENDPOINT_NAME=PEUPEU
ENVIRONMENT=DEV

# ===================
# ALMACENAMIENTO
# ===================
S3_RAW_BUCKET=sofia-dev-datalake-509399624591-us-east-1-raw-s3
DYNAMO_LOGS_TABLE=sofia-dev-datalake-logs-ddb
TOPIC_ARN=arn:aws:sns:us-east-1:509399624591:sofia-dev-datalake-failed-sns

# ===================
# ARCHIVOS DE CONFIGURACIÓN
# ===================
TABLES_CSV_S3=./config/tables.csv
CREDENTIALS_CSV_S3=./config/credentials.csv
COLUMNS_CSV_S3=./config/columns.csv

# ===================
# CONFIGURACIÓN DE PROCESAMIENTO
# ===================
MAX_THREADS=6
CHUNK_SIZE=500000
OUTPUT_FORMAT=parquet
EXTRACTOR_TYPE=sqlserver
LOADER_TYPE=s3
MONITOR_TYPE=dynamodb
